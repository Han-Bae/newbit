<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="inSQL">

	<select id="myInfo" resultType="inVO" parameterType="string">
		SELECT
			nickname, email
		FROM
			account
		WHERE
			id = #{id}
	</select>
	

</mapper>