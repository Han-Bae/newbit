<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dSQL">
	<!-- 자주 사용하는 내용 -->
	<sql id="delShow">
		WHERE isshow ='N'
	</sql>
	
	<delete id="delAccount">
		DELECT FROM account
		<include refid="delShow" />
	</delete>
</mapper>